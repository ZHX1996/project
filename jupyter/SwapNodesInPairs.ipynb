    "def Swap(l):\n",
    "    i = 0\n",
    "    while i < len(l)-1:\n",
    "        k = l[i]\n",
    "        l[i] = l[i+1]\n",
    "        l[i+1] = k\n",
    "        i+=2\n",
    "    return l\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    l = [1,2,3,4,5,6,7,8]\n",
    "    print(Swap(l))   "
   ]
