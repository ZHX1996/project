   "source": [
    "def plusOne(digits):\n",
    "    num = 0\n",
    "    for i in range(len(digits)):\n",
    "        num += digits[i] * pow(10, (len(digits)-1-i))\n",
    "    return [int(i) for i in str(num+1)]\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    digits = [9,9,9,9]\n",
    "    print(plusOne(digits))\n",
    "    "
   ]
