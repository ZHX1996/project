    "def mySqrt(x):\n",
    "    k = []\n",
    "    for i in range(int(x/2)+2):\n",
    "        if i**2 <= x and (i+1)**2 >= x:\n",
    "            k.append(i)\n",
    "    print(k)\n",
    "    return k[-1]\n",
    "            \n",
    "\n",
    "if __name__ == '__main__':\n",
    "    x = 1\n",
    "    print(mySqrt(x))"
   ]
