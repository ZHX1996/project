    "def RemoveElement(l, val):\n",
    "    i = 0\n",
    "    while i < len(l)-1:\n",
    "        if l[i] == val:\n",
    "            del l[i]\n",
    "        else:\n",
    "            i+=1\n",
    "    return l\n",
    "\n",
    "if __name__ == '__main__':\n",
    "    l = [3,3,2,3,4]\n",
    "    val = 3\n",
    "    print(RemoveElement(l, val))"
   ]
