   "source": [
    "from sklearn import svm\n",
    "\n",
    "X = [[0, 0], [1, 1], [1, 0]]\n",
    "y = [0, 1, 1]\n",
    "clf = svm.SVC()\n",
    "clf.fit(X, y)\n",
    "result = clf.predict([[2, 2]])\n",
    "print(result)\n",
    "\n",
    "X1 = [[0, 0], [1, 1]]\n",
    "y1 = [0.5, 1.5]\n",
    "clf1 = svm.SVR()\n",
    "clf1.fit(X1, y1)\n",
    "result1 = clf1.predict([[2, 2]])\n",
    "print(result1)"
