    "def reverse_integer(i):\n",
    "    if i >= 0:\n",
    "        i = str(i)\n",
    "        j = int(i[::-1],10)\n",
    "    else:\n",
    "        i = str(abs(i))\n",
    "        j = -1*int(i[::-1],10)\n",
    "    print(j*(j<2**31))\n",
    "\n",
    "if __name__=='__main__':\n",
    "    i = -1326\n",
    "    reverse_integer(i)"
